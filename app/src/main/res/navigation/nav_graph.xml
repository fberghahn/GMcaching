<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/cachesListFragment">

    <fragment
        android:id="@+id/cachesListFragment"
        android:name="com.example.gmcaching.CachesListFragment"
        android:label="@string/app_name">
        <action
            android:id="@+id/action_cachesListFragment_to_mapsActivity2"
            app:destination="@id/mapsActivity" />
    </fragment>
    <activity
        android:id="@+id/mapsActivity"
        android:name="com.example.gmcaching.MapsActivity"
        android:label="@string/app_name"
        tools:layout="@layout/activity_maps" >
        <argument android:name="latlng"
            app:argType="string" />
    </activity>
</navigation>
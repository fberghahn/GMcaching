<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/cachesListFragment">

    <fragment
        android:id="@+id/cachesListFragment"
        android:name="com.example.gmcaching.CachesListFragment"
        android:label="@string/app_name">
        <action
            android:id="@+id/action_cachesListFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
    </fragment>
    <activity
        android:id="@+id/mapsActivity"
        android:name="com.example.gmcaching.MapsActivity"
        android:label="@string/app_name"
        tools:layout="@layout/activity_maps" >
        <argument android:name="latlng"
            app:argType="string" />
    </activity>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.gmcaching.MapsFragment"
        android:label="@string/cache_list_fragment_label">
        <argument
            android:name="lat"
            app:argType="string" />
        <argument
            android:name="lng"
            app:argType="string" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
</navigation>